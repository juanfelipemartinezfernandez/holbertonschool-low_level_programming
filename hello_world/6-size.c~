#include <stdio.h>

/**
 * main - Entry point
 * Description: 'Print size of types'
 * Return: Always 0 (Success)
 */

int main(void)
{
  printf("Size of a char: %lu",(unsigned long)sizeof(char n));
  printf("Size of a int: %lu",(unsigned long)sizeof(int n));
  printf("Size of a long int: %lu",(unsigned long)sizeof(long int n));
  printf("Size of a long long int: %lu",(unsigned long)sizeof(long long int n));
  printf("Size of a float: %lu",(unsigned long)sizeof(float n));
}
